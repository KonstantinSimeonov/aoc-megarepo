c + d + e - 4 = 0

[1, 1, 0, 1, 0, 0] + -7 = None
a + b + d - 7 = 0

======= 0 =======
e + f - 3 = 0
b + f - 5 = 0
c + d + e - 4 = 0
a + b + d - 7 = 0

